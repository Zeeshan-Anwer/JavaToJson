package a;
//find sum of third and 2nd largest value from the {1,9,7,8,3,0,5,2}

public class Large {

	public static void main(String[] args) {
		int arr[]={1,9,7,8,3,0,5,2};
		int temp1=arr[0];
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]<arr[j])
					temp1=arr[j];
			}
		}
	}

}
